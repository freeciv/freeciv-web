style "help_label"
{
  fg[PRELIGHT] = "red"
  fg[ACTIVE] = "red"
  fg[NORMAL] = "red"
  fg[INSENSITIVE] = "red"
}

style "help_link"
{
  fg[PRELIGHT] = "blue"
  fg[ACTIVE] = "blue"
  fg[NORMAL] = "blue"
  fg[INSENSITIVE] = "blue"
}

style "help_text"
{
  bg[NORMAL] = { 0.9, 0.9, 0.9 }
  base[NORMAL] = { 0.9, 0.9, 0.9 }
  text[NORMAL] = "black"
}

style "chatline"
{
  base[NORMAL] = "white"
  text[NORMAL] = "black"
  base[INSENSITIVE] = "white"
  text[INSENSITIVE] = "black"
}

style "beta_label"
{
  fg[NORMAL] = "red"
  fg[PRELIGHT] = "red"
}

